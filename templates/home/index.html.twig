    <!-- si un message est envoyer via contact us -->
    
    <!-- 1548585 contact effectue -->
    {% if message_recu == 1548585 %}
        <div class="alert alert-success fixed-bottom text-center" id="message_alert" role="alert" style="height: 70px; margin-bottom: 0; font-size: 1.3em; opacity: 0.8;">
            Votre message est envoyée !
        </div>
    <!-- 882528 abonnement effectue -->
    {% elseif message_recu == 882528 %}
        <div class="alert alert-success fixed-bottom text-center" id="message_alert" role="alert" style="height: 70px; margin-bottom: 0; font-size: 1.3em; opacity: 0.8;">
            Vous êtes désormais abonné(e) à notre newsletter
        </div>
    {% endif %}

    <script>
        setTimeout(
        function() 
        {
            $("#message_alert").css('display', 'none');
        }, 3000);
    </script>
    

