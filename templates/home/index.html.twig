                    <div class="section-title text-center title-ex1">
                        <h2>Service</h2>
                        <p>Choisisez l'option qui vous convient</p>
                    </div>
                </div>
            </div>
            <!-- Pricing Table starts --><!-- Pricing Table starts -->
            <div class="row mb-4">
                {% set i = 1 %}
                {% for service in services %}
                        <div class="col-lg-4 col-md-6 col-sm-6 col-xs-12 mb-4">
                            
                            <div class="price-card {% if i % 2 == 0 %}featured{% endif %}">
                                <h2>{{ service.name}}</h2>
                                <p>{{ service.description }}</p>
                                <p class="price"><span>{{ service.price }} + </span></p>
                                <ul class="pricing-offers">
                                    <li>
                                        {% if service.getPageNumber == 0 %}
                                            Nombre de page au choix
                                        {% else %}
                                            {{ service.getPageNumber }} page
                                        {% endif %}    
                                    </li>
                                    {% for carac in service.getCaracteristiques() %}
                                        <li>{{ carac.libele }}</li>
                                    {% endfor %}

                                </ul>
                                <a href="{{ path('template_catalogue', {'service':service.id}) }}" class="btn btn-primary btn-mid">Details</a>
                            </div>
                        </div>
                    {% set i = i + 1 %}
                {% endfor %}
            </div>
        </div>
